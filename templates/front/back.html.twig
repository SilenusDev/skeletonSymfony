<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('front_style') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('front_script') }}
        {% endblock %}
    </head>
    <body>
            {% block menu %}
                {% include 'front/partials/menu.html.twig' %}
            {% endblock %}
       <div class="container-fluid">
            <div class="row">
                <div class="col-2">
                    <h1>Back Admin</h1>
                    <hr>
                    <h3>Modèles</h3>
                    <a href="{{ path('app_back_model_index') }}">Voir</a>
                    <a href="{{ path('app_back_model_new') }}">Créer</a>
                    
                    <hr>
                    <h3>Marques</h3>
                    <a href="{{ path('app_back_brand_index') }}">Voir</a>
                    <a href="{{ path('app_back_brand_new') }}">Créer</a>
                </div>
                <div class="col-10">
                    {% block body %}{% endblock %}
                </div>
            </div>
            
                
            
          
        
    </body>
</html>